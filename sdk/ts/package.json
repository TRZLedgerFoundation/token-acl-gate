{
    "name": "@token-acl/abl-sdk",
    "version": "0.1.3",
    "description": "SDK for Token ACL Allow Block List",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "files": [
      "dist"
    ],
    "scripts": {
      "build": "rimraf dist && tsc",
      "dev": "echo 'Dev will be implemented'",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "lint": "eslint src --ext .ts",
      "lint:fix": "eslint src --ext .ts --fix",
      "type-check": "tsc --noEmit",
      "clean": "rm -rf dist"
    },
    "dependencies": {
      "@trezoa/kit": "^5.0.0"
    },
    "devDependencies": {
      "@types/jest": "^29.5.0",
      "@typescript-eslint/eslint-plugin": "^8.34.0",
      "@typescript-eslint/parser": "^8.34.0",
      "eslint": "^9.32.0",
      "jest": "^29.5.0",
      "ts-jest": "^29.1.0"
    },
    "peerDependencies": {
      "typescript": "^5.0.0"
    },
    "publishConfig": {
      "access": "public",
      "registry": "https://registry.npmjs.org"
    },
    "keywords": [
      "trezoa",
      "token-2022",
      "extensions",
      "core",
      "hooks"
    ]
  }
  